{
  "dashboard": {
    "id": null,
    "title": "ü§ñ Coaching IA - Monitoring Dashboard",
    "tags": ["coaching", "ai", "django", "fastapi"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "üöÄ Vue d'ensemble Services",
        "type": "stat",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{job=~\"django|fastapi|streamlit\"}",
            "legendFormat": "{{job}} Status",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "mappings": [
              {"options": {"0": {"text": "DOWN", "color": "red"}}, "type": "value"},
              {"options": {"1": {"text": "UP", "color": "green"}}, "type": "value"}
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "‚ö° Performance Django API",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(django_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Response Time",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(django_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P50 Response Time", 
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "Seconds", "min": 0}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.5], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "2m",
          "frequency": "10s",
          "handler": 1,
          "name": "Django High Response Time",
          "noDataState": "no_data"
        }
      },
      {
        "id": 3,
        "title": "ü§ñ OpenAI API Performance",
        "type": "graph", 
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(openai_request_duration_seconds_bucket[5m]))",
            "legendFormat": "OpenAI P95 Latency",
            "refId": "A"
          },
          {
            "expr": "rate(openai_requests_total[5m])",
            "legendFormat": "OpenAI Requests/sec",
            "refId": "B"
          }
        ]
      },
      {
        "id": 4,
        "title": "üí∞ Co√ªts OpenAI",
        "type": "stat",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "increase(openai_cost_total_usd[1h])",
            "legendFormat": "Co√ªt/heure ($)",
            "refId": "A"
          },
          {
            "expr": "increase(openai_tokens_total[1h])",
            "legendFormat": "Tokens/heure",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "üîê S√©curit√© - √âchecs Auth",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16}, 
        "targets": [
          {
            "expr": "rate(django_auth_failures_total[5m])",
            "legendFormat": "Auth Failures/sec",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Failures/sec", "min": 0}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.16], "type": "gt"},
              "query": {"params": ["A", "1m", "now"]},
              "reducer": {"type": "avg"},
              "type": "query"
            }
          ],
          "for": "1m",
          "name": "High Auth Failure Rate",
          "message": "Tentatives d'authentification suspectes d√©tect√©es"
        }
      },
      {
        "id": 6,
        "title": "üìä Sessions Coaching",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16},
        "targets": [
          {
            "expr": "rate(coaching_sessions_total[5m])",
            "legendFormat": "Sessions cr√©√©es/sec",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(coaching_session_duration_seconds_bucket[5m]))",
            "legendFormat": "Dur√©e P95 sessions",
            "refId": "B"
          }
        ]
      },
      {
        "id": 7,
        "title": "üíæ Resources Syst√®me",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "(node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes) / node_memory_MemTotal_bytes * 100",
            "legendFormat": "Memory Usage %",
            "refId": "A"
          },
          {
            "expr": "rate(node_cpu_seconds_total{mode=\"user\"}[5m]) * 100",
            "legendFormat": "CPU Usage %",
            "refId": "B"
          }
        ],
        "yAxes": [
          {"label": "Percentage", "max": 100, "min": 0}
        ]
      },
      {
        "id": 8,
        "title": "üóÉÔ∏è Base de Donn√©es",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "django_db_connections_active",
            "legendFormat": "Connexions actives",
            "refId": "A"
          },
          {
            "expr": "django_db_connections_max",
            "legendFormat": "Connexions max",
            "refId": "B"
          }
        ]
      }
    ],
    "time": {"from": "now-1h", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "30s"
  }
}